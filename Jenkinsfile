pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
       echo "build step"
       sleep 10
      }
    }
  }
    stages {
     stage('Test') {
      steps {
       echo "test step"
       sleep 10
      }
    }
   }
    stages {
     stage('Deploy') {
      steps {
       echo "deploy step"
       sleep 10
      }
    }
  }
    stages {
     stage('Docker') {
      steps {
       echo "image step"
       sleep 10
      }
    }
  }
}










